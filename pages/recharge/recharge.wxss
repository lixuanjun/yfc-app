page{background: #f0f0f0;}
view.main_recharge{width: 690rpx;height:450rpx;/*background: orange;*/margin: 0 auto;}
view.main_recharge>text{font-size: 34rpx;color:#141414;}
view.money_wrap{height: 350rpx;margin-top: 30rpx;/*background: yellow;*/display: flex;flex-wrap:wrap;align-content: flex-start;justify-content: space-between;}
view.moneyBtn{width: 210rpx;height: 120rpx;border:1rpx solid #ddd;box-sizing: border-box;border-radius: 10rpx;display: flex;flex-direction: column;justify-content: center;align-items: center;}
view.moneyBtnSelected{background: #f46246;border:1rpx solid #f46246;color:#fff;}

view.moneyBtn:nth-child(1){margin-bottom: 30rpx;}
view.moneyBtn>view{font-size: 36rpx;color:#9b9b9b;}
view.moneyBtn>text{font-size: 30rpx;color:#78a8ed;}
view.moneyBtnSelected>view,view.moneyBtnSelected>text{color:#fff;}/*优先级问题，需要放在后面*/
view.moneyBtn>input{width: 100%;height: 100%;font-size:40rpx;color:#f46246;text-align: center;}

/* 立即充值按钮 */
view.btn_recharge{width: 690rpx;height: 100rpx;background: #78a8ed;margin: 0 auto;border-radius: 20rpx;font-size: 40rpx;color:#fff;text-align: center;line-height: 100rpx;}

/* 去充电按钮，如果有设备编码 */
view.btn_goCharging{width: 690rpx;height: 100rpx;background: #f46246;margin: 30rpx auto 0;border-radius: 20rpx;font-size: 40rpx;color:#fff;text-align: center;line-height: 100rpx;}





