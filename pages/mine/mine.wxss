/* header */
view.header_wrap{width: 100%;height: 290rpx;background: #78a8ed;}
view.header{width: 100%;height: 140rpx;/*background: orange;*/display: flex;}
view.photo{width: 120rpx;height: 100%;/*background: yellow;*/display: flex;align-items: center;margin-left: 30rpx;overflow: hidden;}
view.photo>image{width: 120rpx;height: 120rpx;}
view.infor{width: 60%;height: 100%;/*background: green;*/font-size:30rpx;color:#fff;display: flex;flex-direction: column;justify-content: center;}
view.infor>view{margin-left:30rpx ;margin-top: 10rpx;}
view.infor>text{margin-left:30rpx ;}
view.infor>view>text:nth-child(1){margin-right: 40rpx;}
view.menu{width:100%;height: 150rpx;/*background: pink;*/border-bottom: 1rpx solid #999;box-sizing: border-box;display: flex;justify-content:space-around;}
view.menu>view{display: flex;flex-direction: column;justify-content: center;align-items: center;}
view.menu>view>image{width: 64rpx;height: 64rpx;}
view.menu>view>text{font-size:30rpx;color:#fff;}
/* gray */
view.gray{width: 100%;height: 30rpx;background: #eee;}
/* lists */
view.lists>view{width: 100%;height: 98rpx;/*background: orange;*/border-bottom: 1rpx solid #eee;box-sizing: border-box;display: flex;justify-content: space-between;align-items: center;font-size:30rpx;color:#999;}
view.lists_left{display: flex;align-items: center;}
view.lists_left>image{width: 40rpx;height: 40rpx;margin: 0 16rpx 0 30rpx;}
view.lists_right{display: flex;align-items: center;}
view.lists_right>image{width: 14rpx;height: 26rpx;margin:0 30rpx 0 10rpx;}





